{
  "ignition": {
    "config": {},
    "timeouts": {},
    "version": "2.1.0"
  },
  "networkd": {},
  "passwd": {
    "users": [
      {
        "name": "core",
        "sshAuthorizedKeys": [
          "ssh-rsa "
        ]
      }
    ]
  },
  "storage": {
  "disks": [
    {
      "device": "/dev/sda",
      "partitions": [
        {
          "label": "ROOT"
        }
      ],
      "wipeTable": true
    }
  ],
  "filesystems": [
    {
      "mount": {
        "device": "/dev/disk/by-partlabel/ROOT",
        "format": "ext4",
        "label": "ROOT",
        "wipeFilesystem": true
      }
    }
  ],
  "files": [{
      "filesystem": "root",
      "path": "/etc/hostname",
      "mode": 420,
      "contents": { "source": "data:,rey" }
    }]
  },
  "systemd": {
    "units": [
      {
        "enable": true,
        "name": "etcd2.service"
      }
    ]
  }
}
